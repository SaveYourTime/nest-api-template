PORT=5000
WEB_HOST=http://localhost:3000
ADMIN_HOST=http://localhost:4000
ACCESS_CONTROL_ALLOW_CREDENTIALS=true

# DB_HOST could be either the name of your db service in the docker-compose.yml (Such as: `mysql`)
# or `host.docker.internal` when you expose the db port to the host by setting '${DB_PORT}:${DB_PORT}'
DB_HOST=mysql
DB_PORT=3306
DB_USER=aaron
DB_PASS=123
DB_ROOT_PASS=12345
DB_NAME=awesome
DB_DROP=false
DB_SYNC=true
